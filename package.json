{
  "name": "my-project",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "db:studio": "pnpm drizzle-kit studio --verbose",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,js,jsx}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json,js,jsx}\"",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation eslint .",
    "lint:debug": "cross-env NODE_OPTIONS=--no-deprecation eslint . --debug",
    "lint:debug:prettier": "cross-env NODE_OPTIONS=--no-deprecation  pnpm dlx eslint-config-prettier src/app/layout.tsx",
    "lint:debug:timing": "cross-env NODE_OPTIONS=--no-deprecation TIMING=1 eslint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation eslint . --fix",
    "lint:inspect": "pnpm dlx @eslint/config-inspector@latest",
    "lint:next": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "packages:sort": "pnpm dlx sort-package-json",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "shad": "pnpm dlx shadcn@latest add",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:e2e:codegen": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pmpm exec playwright codegen",
    "test:e2e:debug": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pmpm exec playwright test --debug",
    "test:e2e:report": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pmpm exec playwright show-report",
    "test:e2e:ui": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pmpm exec playwright test --ui",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --browser.headless",
    "test:watch": "cross-env NODE_OPTIONS=--no-deprecation vitest watch --browser.headless",
    "test:watch:ui": "cross-env NODE_OPTIONS=--no-deprecation vitest watch --ui",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@payloadcms/admin-bar": "3.47.0",
    "@payloadcms/db-sqlite": "3.47.0",
    "@payloadcms/live-preview-react": "3.47.0",
    "@payloadcms/next": "3.47.0",
    "@payloadcms/payload-cloud": "3.47.0",
    "@payloadcms/plugin-form-builder": "3.47.0",
    "@payloadcms/plugin-nested-docs": "3.47.0",
    "@payloadcms/plugin-redirects": "3.47.0",
    "@payloadcms/plugin-search": "3.47.0",
    "@payloadcms/plugin-seo": "3.47.0",
    "@payloadcms/richtext-lexical": "3.47.0",
    "@payloadcms/ui": "3.47.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tailwindcss/postcss": "^4.1.11",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "17.2.0",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "lucide-react": "^0.525.0",
    "next": "15.4.1",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "payload": "3.47.0",
    "prism-react-renderer": "^2.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.60.0",
    "sharp": "0.34.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.31.0",
    "@next/eslint-plugin-next": "^15.4.1",
    "@playwright/test": "1.54.1",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/react": "16.3.0",
    "@types/escape-html": "^1.0.2",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "24.0.14",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@typescript-eslint/utils": "^8.37.0",
    "@vitejs/plugin-react": "4.6.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.19",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "copyfiles": "^2.4.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.31.0",
    "eslint-config-next": "15.4.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "6.0.0-rc1",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.3.0",
    "jiti": "^2.4.2",
    "jsdom": "26.1.0",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.37.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4",
    "vitest-browser-react": "^1.0.0"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  }
}
